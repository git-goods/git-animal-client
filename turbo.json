{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:web": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env",
        ".env.*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev:web": {
      "cache": false,
      "persistent": true
    }
  }
}