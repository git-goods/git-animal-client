{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "prepare": {
      "dependsOn": [
        "^prepare"
      ],
      "outputs": [
        "styled-system/**"
      ]
    },
    "build": {
      "dependsOn": [
        "^build",
        "^prepare"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env",
        ".env.*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        ".vercel/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": [
    "NODE_ENV"
  ]
}