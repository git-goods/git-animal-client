{
  "name": "@gitanimals/ui-tailwind",
  "version": "0.0.1",
  "description": "Tailwind CSS UI components and theme for GitAnimals",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "import": "./dist/preset.mjs",
      "require": "./dist/preset.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts", 
      "import": "./dist/components/index.mjs",
      "require": "./dist/components/index.js"
    },
    "./theme": {
      "types": "./dist/theme/index.d.ts",
      "import": "./dist/theme/index.mjs",
      "require": "./dist/theme/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@gitanimals/ui-token": "workspace:*",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@gitanimals/eslint-config": "workspace:*",
    "@gitanimals/typescript-config": "workspace:*",
    "@types/node": "^20",
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "*",
    "react-dom": "*",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.0.2",
    "typescript": "*"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^4.0.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/git-animals/gitanimals.git"
  },
  "license": "MIT"
}